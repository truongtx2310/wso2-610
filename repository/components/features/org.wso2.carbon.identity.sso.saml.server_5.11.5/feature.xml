<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature id="org.wso2.carbon.identity.sso.saml.server" label="Identity SAML Single Sign-on Server Feature" provider-name="%providerName" version="5.11.5">
  <description>This feature contains the core bundles required for Back-end SAML2.0 based Single
        Sign-on functionality</description>
  <copyright>%copyright</copyright>
  <license url="%licenseURL">%license</license>
  <plugin id="joda-time" unpack="false" version="2.9.4.wso2v1"/>
  <plugin id="org.wso2.carbon.identity.sso.saml.common" unpack="false" version="5.11.5"/>
  <plugin id="org.wso2.carbon.identity.sso.saml" unpack="false" version="5.11.5"/>
  <plugin id="opensaml" unpack="false" version="3.3.1.wso2v7"/>
  <require>
    <import feature="org.wso2.carbon.identity.application.mgt.server" match="compatible" version="5.23.14"/>
    <import feature="org.wso2.carbon.identity.core" match="compatible" version="5.23.14"/>
    <import feature="org.wso2.carbon.identity.application.authentication.framework.server" match="compatible" version="5.23.14"/>
    <import feature="org.wso2.carbon.identity.saml.common.util" match="equivalent" version="1.3.0"/>
    <import feature="org.wso2.carbon.idp.mgt.server" match="compatible" version="5.23.14"/>
    <import feature="org.wso2.carbon.core" match="compatible" version="4.9.0"/>
  </require>
</feature>
